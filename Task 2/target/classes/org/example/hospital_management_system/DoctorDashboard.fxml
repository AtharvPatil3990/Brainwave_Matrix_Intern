<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.hospital_management_system.DoctorDashboardController">
   <children>
      <Label layoutX="305.0" layoutY="14.0" text="Doctor's Dashboard">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="welcomeLabel" layoutX="287.0" layoutY="84.0" prefHeight="27.0" prefWidth="245.0" text="Welcome, ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="viewAppointmentButton" layoutX="332.0" layoutY="182.0" mnemonicParsing="false" onAction="#onViewAppointmentClick" prefHeight="50.0" prefWidth="137.0" text="View Appointments" />
      <Button fx:id="viewPatientEHRButton" layoutX="329.0" layoutY="275.0" mnemonicParsing="false" onAction="#onViewPatientEHRClick" prefHeight="50.0" prefWidth="137.0" text="View Patients EHR" />
      <Button fx:id="logoutButton" layoutX="332.0" layoutY="376.0" mnemonicParsing="false" onAction="#onLogoutClick" prefHeight="50.0" prefWidth="137.0" text="Logout" />
   </children>
</AnchorPane>
